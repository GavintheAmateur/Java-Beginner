import java.util.*;

public class Prime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner in = new Scanner(System.in);
		int input = in.nextInt();
		boolean isPrime = true;
		for (int j = 2; j < input; j ++)  {
			if (input % j ==0 )  {
				isPrime = false;
				break;
			}
		}
		System.out.println(isPrime);
		
	}

}
